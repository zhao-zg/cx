{
  "name": "conference-info",
  "version": "1.0.0",
  "description": "特会信息合集 - 安卓应用",
  "scripts": {
    "build": "python main.py",
    "build:secure": "python build_secure.py",
    "encrypt:app-update": "python encrypt_app_update.py",
    "restore:app-update": "python encrypt_app_update.py --restore",
    "cap:init": "npx cap init",
    "cap:add": "npx cap add android",
    "cap:sync": "npx cap sync",
    "cap:open": "npx cap open android",
    "android:build": "npm run build && npm run cap:sync && cd android && ./gradlew assembleRelease",
    "android:build:secure": "npm run build:secure && npm run cap:sync && cd android && ./gradlew assembleRelease",
    "android:dev": "npm run build && npm run cap:sync && npm run cap:open",
    "restore:dev": "python build_secure.py --restore"
  },
  "dependencies": {
    "@capacitor/core": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor-community/text-to-speech": "^5.1.0",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@capacitor/android": "^6.0.0",
    "javascript-obfuscator": "^4.1.0"
  }
}
